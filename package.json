    "express": "^4.17.1",
    "mocha": "^6.2.1",
    "moment": "^2.29.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-bootstrap-carousel": "^4.1.1",
